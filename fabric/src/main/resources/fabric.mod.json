{
    "schemaVersion": 1,
    "id": "mod_id",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://www.griefed.de",
        "sources": "https://github.com/Griefed/AddEmAll",
        "issues": "https://github.com/Griefed/AddEmAll/issues"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "de.griefed.addemall.AddEmAllFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "1.19.2",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  